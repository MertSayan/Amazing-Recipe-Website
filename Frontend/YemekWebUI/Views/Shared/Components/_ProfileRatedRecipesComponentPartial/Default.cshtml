@model List<ResultRatedRecipeDto>
<div class="recepie_area plus_padding">
    <h1 class="recepie_area plus_padding">Beğendiğim Tarifler </h1>
    <br />
    <br />
    <br />
    <div class="container">
        <div class="row">
            @foreach (var item in Model)
            {
                
                <div class="col-xl-4 col-lg-4 col-md-6">
                    <div class="single_recepie text-center">
                        <div class="recepie_thumb">
                            <img src="@item.RecipeImageUrl" alt="">
                        </div>
                        <h3>@item.Title</h3>
                         <labe>Verdiğim Puan:<span>@item.Score</span></labe>
                        <a href="#" class="line_btn">Tarifin Tamamını Gör</a>
                    </div>
                </div>
            }
        </div>
    </div>
</div>